{
  "env": {
    "test": {
      "presets": [
        "env",
        "es2015",
        "react",
        "stage-2"
      ],
      "plugins": [
        "transform-class-properties",
        "syntax-object-rest-spread"
      ]
    },
    "node": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": "9.5"
            },
            "debug": true
          }
        ],
        "es2015",
        "react",
        "stage-2"
      ],
      "plugins": [
        "transform-class-properties",
        "syntax-object-rest-spread"
      ]
    }
  }
}