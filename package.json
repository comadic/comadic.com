{
  "name": "comadic.com",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint src",
    "prettier": "prettier --write \"src/**/*.js\" .eslintrc.js",
    "flow": "flow src",
    "test": "NODE_ENV=test jest --config jest.config.js",
    "test-watch": "yarn run test --watch",
    "test-coverage": "yarn run test --coverage",
    "verify": "yarn run lint && yarn run flow && yarn run test",
    "dev": "cross-env NODE_ENV=development nodemon --config ./nodemon.json",
    "start": "npm run build && cross-env NODE_ENV=production PORT=8081 forever start --minUptime 1000 --spinSleepTime 1000 public/compiled/server.js",
    "stop": "forever stop public/compiled/server.js",
    "clean": "rm -rf public",
    "build-dev": "cross-env NODE_ENV=development webpack --debug --colors --display-error-details --config ./webpack/webpack.config.js",
    "build": "npm run clean && cross-env NODE_ENV=production npm run webpack -- --env.browser && cross-env NODE_ENV=production npm run webpack -- --env.server",
    "webpack": "webpack --colors --display-error-details --config ./webpack/webpack.config.js"
  },
  "devDependencies": {
    "autoprefixer": "^8.4.1",
    "babel-cli": "^6.26.0",
